  "puzzle": {
    "targetLine": {
      "character": "SMÉAGOL",
      "text": "Master — Master looks after us. Master wouldn't hurt us.",
      "movie": "The Two Towers"
    },
    "contextBefore": {
      "character": "SAM",
      "text": "We're not gonna wait for you. Come on."
    },
    "contextAfter": [
      {
        "character": "GOLLUM",
        "text": "Master broke his promise!"
      },
      {
        "character": "SMÉAGOL",
        "text": "Don't ask Sméagol. Poor, poor Sméagol."
      },
      {
        "character": "GOLLUM",
        "text": "Master betrayed us! Wicked, tricksy, false! We ought to ring his filthy little neck!"
      },
      {
        "character": "GOLLUM",
        "text": "Kill him! Kill him! Kill them both. Then we take the Precious and we be the master."
      },
      {
        "character": "SMÉAGOL",
        "text": "But the fat hobbit. He knows. Eyes always watching."
      }
    ]
  },
